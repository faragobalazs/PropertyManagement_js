generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url = env("DATABASE_URL")
}

model Building {
  id          Int @id @default(autoincrement())
  name        String
  address     String
  year        Int
  levels      Int
  webpage     String
  email       String
  phone       String

  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}

